msgstr "Moderne Wohnungsrenovierung"

#: .\main\templates\main\home.html:18
msgid "Утепление фасадов"
msgstr "Fassadendämmung"

#: .\main\templates\main\home.html:19 .\main\templates\main\home.html:44
msgid "Фасадные работы"
msgstr "Fassadenarbeiten"

